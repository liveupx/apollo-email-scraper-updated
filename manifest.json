{
  "name": "Apollo Easy Scrape by Liveupx.com",
  "description": "Easily scrape data from web pages with a single click.",
  "version": "1.1",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "clip.png",
    "48": "clip.png",
    "128": "clip.png"
  },
  "content_scripts": [
    {
      "matches": ["https://app.apollo.io/*"],
      "js": ["content_script.js"]
    }
  ]
}
